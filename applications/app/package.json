  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"webpack/config/tsconfig.json\" webpack --config webpack/config/production.ts",
    "bundlesize": "bundlesize",
    "clean": "tsc --build --clean",
    "compile": "tsc --build",
    "fmt": "prettier",
    "fmt:all": "yarn fmt .",
    "fmt:diff": "pretty-quick",
    "lint": "run-s lint:es lint:md",
    "lint:all": "run-s lint:es:all lint:md:all",
    "lint:diff": "run-s lint:es:diff lint:md:diff",
    "lint:es": "eslint",
    "lint:es:all": "yarn lint:es \"**/*\"",
    "lint:es:diff": "eslint-changeset",
    "lint:md": "markdownlint",
    "lint:md:all": "yarn lint:md \"**/*.md\"",
    "lint:md:diff": "yarn lint:md:all",
    "precommit": "lint-staged",
    "prepack": "yarn build",
    "pretest": "yarn compile",
    "start": "cross-env TS_NODE_PROJECT=\"webpack/config/tsconfig.json\" webpack-dev-server --config webpack/config/development.ts",
    "test": "jest --config .jestrc.json",
    "test:ci": "yarn test --ci",
    "test:ci:branch": "yarn test:ci --changedSince master"
  },
