  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"webpack/config/tsconfig.json\" webpack --config webpack/config/prod.ts",
    "bundlesize": "bundlesize",
    "compile": "tsc",
    "lint": "yarn lint:es",
    "lint:es": "eslint",
    "precommit": "lint-staged",
    "test": "jest --config .jestrc.json",
    "test:ci": "yarn test --ci",
    "test:coverage": "yarn test --coverage",
    "test:precommit": "yarn test --bail --findRelatedTests",
    "test:prepush": "yarn test --bail",
    "test:pull-request": "yarn test --ci --changedSince=\"master\"",
    "watch": "cross-env TS_NODE_PROJECT=\"webpack/config/tsconfig.json\" TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling webpack-dev-server --config webpack/config/dev.ts"
  },
