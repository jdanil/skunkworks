  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"webpack/config/tsconfig.json\" webpack --config webpack/config/production.ts",
    "bundlesize": "bundlesize",
    "clean": "tsc --build --clean",
    "compile": "tsc --build",
    "cy:ci": "start-server-and-test \"yarn start\" 8080 \"yarn cy:run\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "fmt": "prettier",
    "fmt:all": "yarn fmt .",
    "fmt:all:check": "yarn fmt:all --check",
    "fmt:all:write": "yarn fmt:all --write",
    "fmt:diff": "pretty-quick",
    "fmt:diff:check": "yarn fmt:diff --check",
    "fmt:diff:write": "yarn fmt:diff",
    "lint": "run-s lint:es lint:md",
    "lint:all": "run-s lint:es:all lint:md:all",
    "lint:diff": "run-s lint:es:diff lint:md:diff",
    "lint:es": "eslint",
    "lint:es:all": "yarn lint:es \"**/*\"",
    "lint:es:diff": "eslint-changeset",
    "lint:md": "markdownlint",
    "lint:md:all": "yarn lint:md \"**/*.md\"",
    "lint:md:diff": "yarn lint:md:all",
    "precommit": "lint-staged",
    "prepack": "yarn build",
    "pretest": "yarn compile",
    "start": "cross-env TS_NODE_PROJECT=\"webpack/config/tsconfig.json\" webpack serve --config webpack/config/development.ts",
    "test": "jest --config .jestrc.json",
    "test:ci": "yarn test --ci",
    "test:ci:branch": "yarn test:ci --changedSince master"
  },
