{
  "transform": {
    "\\.(ts|tsx)": "ts-jest"
  },
  "moduleFileExtensions": ["ts", "tsx", "js"],
  "testRegex": "(<rootDir>/src/.*|(\\.|/)(test))\\.tsx?$",
  "moduleNameMapper": {
    "\\.(s?css|less)$": "<rootDir>/testutils/style-mock.ts"
  },
  "resolver": null,
  "coverageReporters": ["html", "lcov"],
  "setupFiles": ["<rootDir>/testutils/setup-tests.ts"],
  "snapshotSerializers": ["enzyme-to-json/serializer"]
}
