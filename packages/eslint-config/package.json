{
  "name": "@package/eslint-config",
  "version": "0.1.0",
  "private": true,
  "description": "> TODO: description",
  "license": "MIT",
  "main": "index.yaml",
  "files": [
    "**/*.yaml"
  ],
  "scripts": {
    "fmt": "prettier",
    "fmt:all": "yarn fmt .",
    "fmt:all:check": "yarn fmt:all --check",
    "fmt:all:write": "yarn fmt:all --write",
    "fmt:diff": "pretty-quick",
    "fmt:diff:check": "yarn fmt:diff --check",
    "fmt:diff:write": "yarn fmt:diff",
    "lint": "yarn lint:md",
    "lint:all": "yarn lint:md:all",
    "lint:diff": "yarn lint:md:diff",
    "lint:md": "markdownlint",
    "lint:md:all": "yarn lint:md \"**/*.md\"",
    "lint:md:diff": "yarn lint:md:all",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "eslint": "^7.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-disable": "2.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-functional": "3.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-jest-dom": "3.2.3",
    "eslint-plugin-jsdoc": "30.5.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-monorepo": "0.2.1",
