{
  "name": "@package/jest-config",
  "version": "0.1.0",
  "private": true,
  "description": "> TODO: description",
  "license": "MIT",
  "main": "jest-preset.js",
  "files": [
    "**/*.js"
  ],
  "scripts": {
    "fmt": "prettier",
    "fmt:all": "yarn fmt .",
    "fmt:all:check": "yarn fmt:all --check",
    "fmt:all:write": "yarn fmt:all --write",
    "fmt:diff": "pretty-quick",
    "fmt:diff:check": "yarn fmt:diff --check",
    "fmt:diff:write": "yarn fmt:diff",
    "lint": "yarn lint:md",
    "lint:all": "yarn lint:md:all",
    "lint:diff": "yarn lint:md:diff",
    "lint:md": "markdownlint",
    "lint:md:all": "yarn lint:md \"**/*.md\"",
    "lint:md:diff": "yarn lint:md:all",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "jest": "^26.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "jest-extended": "0.11.5",
    "ts-jest": "26.3.0"
  },
  "devDependencies": {
    "lint-staged": "10.3.0",
    "markdownlint-cli": "0.23.2",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2"
  }
}
