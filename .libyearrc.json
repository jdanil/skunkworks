{
  "overrides": {
    "^eslint-plugin-filenames$": {
      "pulse": 4
    },
    "^npm-run-all$": {
      "pulse": 4
    }
  },
  "threshold": {
    "drift": {
      "collective": 1,
      "individual": 0.5
    },
    "pulse": {
      "collective": 50,
      "individual": 2
    },
    "releases": {
      "collective": 25,
      "individual": 10
    },
    "major": {
      "individual": 2
    }
  }
}
